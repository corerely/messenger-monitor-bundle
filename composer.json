{
    "name": "corerely/messenger-monitor-bundle",
    "description": "Monitor messenger messages",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Corerely\\MessengerMonitorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corerely\\MessengerMonitorBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Andrii Oprysko",
            "email": "anjey.link@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "symfony/config": "^6.0|^7.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/event-dispatcher": "^6.0|^7.0",
        "symfony/framework-bundle": "^6.0|^7.0",
        "symfony/http-foundation": "^6.0|^7.0",
        "symfony/http-kernel": "^6.0|^7.0",
        "symfony/messenger": "^6.0|^7.0",
        "symfony/routing": "^6.0|^7.0",
        "symfony/twig-bundle": "^6.0|^7.0",
        "twig/twig": "^2.14.7|^3.0"
    },
    "require-dev": {
        "slevomat/coding-standard": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "allow-plugins": true
    }
}
